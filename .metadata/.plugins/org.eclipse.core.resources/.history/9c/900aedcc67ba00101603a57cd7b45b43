package GarciaManuel13;

import java.util.Scanner;
public class Ejercicio5 

{

	public static void main(String[] args)
	
	{
		double num1;
		double num2;
		
		Scanner programa = new Scanner (System.in);
		
		System.out.print ("Introduce el primer número de su elección: ");
		num1 = programa.nextDouble();
		
		System.out.print ("Introduce el segundo número de su elección: ");
		num2 = programa.nextDouble();
		
		double comparar = Math.min ( num1, num2);
		System.out.println ("\nEl numero menor es: " + comparar);
		
		double potencia = Math.pow ( num1, num2);
		System.out.println ("\nEl primer numero elevado al segundo es: " + potencia);
		
		double raiz = Math.sqrt (num1);
		System.out.println ("\nLa raiz del primer número es: " + raiz);
		
		double random = (double)(Math.random() * (100 - 50));
		System.out.println ("\nEl valor random calculado al segundo número es: " + random);
			
		
			
		programa.close();
	
	}

}
