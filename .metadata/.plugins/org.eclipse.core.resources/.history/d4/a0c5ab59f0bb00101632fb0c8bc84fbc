package GarciaManuel26;

import java.util.Scanner;

public class Ejercicio1

{

	public static void main(String[] args)

	{
		double num1;
		double num2;

		Scanner programa = new Scanner(System.in);

		System.out.print("Indique un número aleatorio:");
		num1 = programa.nextDouble();

		System.out.print("Indique otro número aleatorio: ");
		num2 = programa.nextDouble();

		if (num1 > num2) {
			System.out.println( num1 + "\n Es mayor que " + num2 );
		} else if (num2 > num1) {
			System.out.println( num2 + "\n Es mayor que " + num1 );
		} else {
			System.out.println( num1 + " tiene el mismo valor que " + num2);
		}

		
		programa.close();
	}
		
	}
