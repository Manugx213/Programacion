

package Metodos;



public class Metodo2

{

	public static void main(String[] args) 
	
	{
		int[] vector = {5,10,15};
		
		for (int i=0; i<vector.length; i++) {
			System.out.print( vector [i] + "   " );
		}
		
		inivector(vector);
		
		System.out.print("\nDespues de llamar al mÃ©todo...");
		for (int i=0; i<vector.length; i++) {
			System.out.print( vector [i] + "   " );
		}
		
		
	}
	
	public static void inivector ( int[] paramV ) 
	{
		paramV [0]= 50;
		paramV [1]= 100;
	}

}
