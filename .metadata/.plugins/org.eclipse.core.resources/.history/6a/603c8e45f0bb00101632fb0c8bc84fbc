package GarciaManuel26;

import java.util.Scanner;

public class Ejercicio1

{

	public static void main(String[] args)

	{
		double num1;
		double num2;

		Scanner programa = new Scanner(System.in);

		System.out.println("Indique un número aleatorio:");
		num1 = programa.nextDouble();

		System.out.println("Indique otro número aleatorio: ");
		num2 = programa.nextDouble();

		if (num1 > num2) {
			System.out.println("El primer numero introducido es mayor.");
		} else if (num2 > num1) {
			System.out.println("El segundo numero introducido es mayor.");
		} else {
			System.out.println("Ambos numeros son iguales");
		}

		
		
		programa.close();
	}
		
	}
